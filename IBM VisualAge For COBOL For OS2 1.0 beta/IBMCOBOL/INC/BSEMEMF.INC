ifndef __BSEMEMF__
PAG_READ	EQU	00000001H U      
PAG_WRITE	EQU	00000002H U      
PAG_EXECUTE	EQU	00000004H U      
PAG_GUARD	EQU	00000008H U      
PAG_DEFAULT	EQU	00000400H U      
PAG_COMMIT	EQU	00000010H U      
PAG_DECOMMIT	EQU	00000020H U      
OBJ_TILE	EQU	00000040H U      
OBJ_PROTECTED	EQU	00000080H U      
OBJ_GETTABLE	EQU	00000100H U      
OBJ_GIVEABLE	EQU	00000200H U      
fPERM	EQU	(PAG_EXECUTE  OR  PAG_READ  OR  PAG_WRITE)
fSHARE	EQU	(OBJ_GETTABLE  OR  OBJ_GIVEABLE)
fALLOC	EQU	(OBJ_TILE  OR  PAG_COMMIT  OR  fPERM)
fALLOCSHR	EQU	(OBJ_TILE  OR  PAG_COMMIT  OR  fSHARE  OR  fPERM)
fGETNMSHR	EQU	(fPERM)
fGETSHR	EQU	(fPERM)
fGIVESHR	EQU	(fPERM)
fSET	EQU	(PAG_COMMIT + PAG_DECOMMIT + PAG_DEFAULT + fPERM)
DOSSUB_INIT	EQU	01H U           
DOSSUB_GROW	EQU	02H U           
DOSSUB_SPARSE_OBJ	EQU	04H U           
DOSSUB_SERIALIZE	EQU	08H U           
PAG_SHARED	EQU	00002000H U     
PAG_FREE	EQU	00004000H U     
PAG_BASE	EQU	00010000H U     
