/*
 * -- The C++ Wrapper Class for WPPointer
 */
class WPPointer : public WPDataFile
{
public:

// WPPointer::new creates the class object if necessary, and then uses somNewNoInit
// to allocate memory and create the object. Initialization is in ctors.
void *operator new(size_t size)
{
   SOM_IgnoreWarning(size);
   if (!_WPPointer) WPPointerNewClass(WPPointer_MajorVersion,WPPointer_MinorVersion);
   return (void*)
      SOM_Resolve(_WPPointer,SOMClass,somNewNoInit)
         ((SOMClass *)((void*)_WPPointer));
}

// WPPointer::delete uses the default deallocator for the object's class.
void operator delete(void * obj)
{
   somToken objCls = obj ? *(void**)obj : (somToken) NULL;
   if (somIsObj(objCls)) {
      SOM_Resolve(objCls,SOMClass,somDeallocate)
         ((SOMClass*)objCls, (string)obj);
   }
}

WPPointer()
{
   if (*(void**)this != 
       ((somParentMtabStructPtr)
        (WPPointerCClassData.parentMtab))->mtab)
      return;
   somDefaultInit(0);
}

WPPointer(WPPointer* fromObj)
{
   if (*(void**)this != 
       ((somParentMtabStructPtr)
        (WPPointerCClassData.parentMtab))->mtab)
      return;
   somDefaultCopyInit(0,((SOMObject*)((void*)fromObj)));
}

WPPointer(const WPPointer* fromObj)
{
   if (*(void**)this != 
       ((somParentMtabStructPtr)
        (WPPointerCClassData.parentMtab))->mtab)
      return;
   somDefaultConstCopyInit(0,((SOMObject*)((void*)fromObj)));
}

~WPPointer()
{
   if (!somIsObj(this)) return;
   somDestruct(0,0);
   *(void**)this = **(void***)this;
}


