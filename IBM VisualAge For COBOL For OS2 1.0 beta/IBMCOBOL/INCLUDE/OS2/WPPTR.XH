public:

// M_WPPointer::new creates the class object if necessary, and then uses somNewNoInit
// to allocate memory and create the object. Initialization is in ctors.
void *operator new(size_t size)
{
   SOM_IgnoreWarning(size);
   if (!_M_WPPointer) M_WPPointerNewClass(M_WPPointer_MajorVersion,M_WPPointer_MinorVersion);
   return (void*)
      SOM_Resolve(_M_WPPointer,SOMClass,somNewNoInit)
         ((SOMClass *)((void*)_M_WPPointer));
}

// M_WPPointer::delete uses the default deallocator for the object's class.
void operator delete(void * obj)
{
   somToken objCls = obj ? *(void**)obj : (somToken) NULL;
   if (somIsObj(objCls)) {
      SOM_Resolve(objCls,SOMClass,somDeallocate)
         ((SOMClass*)objCls, (string)obj);
   }
}

M_WPPointer()
{
   if (*(void**)this != 
       ((somParentMtabStructPtr)
        (M_WPPointerCClassData.parentMtab))->mtab)
      return;
   somDefaultInit(0);
}

M_WPPointer(M_WPPointer* fromObj)
{
   if (*(void**)this != 
       ((somParentMtabStructPtr)
        (M_WPPointerCClassData.parentMtab))->mtab)
      return;
   somDefaultCopyInit(0,((SOMObject*)((void*)fromObj)));
}

M_WPPointer(const M_WPPointer* fromObj)
{
   if (*(void**)this != 
       ((somParentMtabStructPtr)
        (M_WPPointerCClassData.parentMtab))->mtab)
      return;
   somDefaultConstCopyInit(0,((SOMObject*)((void*)fromObj)));
}

~M_WPPointer()
{
   if (!somIsObj(this)) return;
   somDestruct(0,0);
   *(void**)this = **(void***)this;
}


